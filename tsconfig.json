{
  "compilerOptions": { //编译选项
    "target": "es2016",       
    "module": "commonjs",
    "lib": ["ES2016"],
    "outDir": "./dist",
    "strictNullChecks": true,
    "removeComments": true,
    "strictPropertyInitialization":true,
    "moduleResolution": "node",
    "noEmitOnError": true,
    "esModuleInterop": true,
    "noImplicitUseStrict": true,
  },
  "include": ["./src"]
}
